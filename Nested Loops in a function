def fun(rows,columns):
    for row in range(rows):
        if row%2 == 0:
            for column in range(1,columns):
                if column%2 == 1:
                    if column != rows:
                        print(" ", end='')
                    else:
                        print(" ")
                else:
                    print("|",end='')
        else:
            print("_____")
fun(5,6)
